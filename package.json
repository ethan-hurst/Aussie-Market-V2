{
	"name": "aussie-market-v2",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "npm run generate:sentry-config && vite build",
		"build:sentry": "npm run generate:sentry-config && vite build && sentry-cli sourcemaps upload --release $npm_package_version --auth-token $SENTRY_AUTH_TOKEN",
		"generate:sentry-config": "node scripts/generate-sentry-config.js",
		"dev": "vite dev",
		"dev:e2e": "dotenv -e .env.e2e -- vite dev --port 5173 --host",
		"preview": "vite preview",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:integration": "vitest run tests/integration/",
		"test:error-handling": "vitest run tests/integration/error-handling-integration.spec.ts tests/integration/error-boundary-integration-simplified.spec.ts tests/integration/payment-error-scenarios-simplified.spec.ts tests/integration/webhook-error-validation.spec.ts",
		"test:coverage": "vitest run --coverage",
		"test:validate-errors": "node scripts/validate-error-handling.js",
		"e2e": "dotenv -e .env.e2e -- playwright test",
		"e2e:ui": "dotenv -e .env.e2e -- playwright test --ui",
		"e2e:debug": "dotenv -e .env.e2e -- playwright test --debug",
		"lhci:ci": "lhci autorun --upload.target=temporary-public-storage",
		"lhci:server": "lhci server --port=9001",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"supabase:start": "supabase start",
		"supabase:stop": "supabase stop",
		"supabase:status": "supabase status",
		"db:reset": "supabase db reset",
		"db:push": "supabase db push",
		"db:diff": "supabase db diff",
		"db:test": "node scripts/run-db-tests.js",
		"sentry:release": "sentry-cli releases new $npm_package_version",
		"sentry:upload": "sentry-cli sourcemaps upload --release $npm_package_version --auth-token $SENTRY_AUTH_TOKEN",
		"sentry:finalize": "sentry-cli releases finalize $npm_package_version",
		"sentry:validate": "node scripts/validate-sentry.js"
	},
	"devDependencies": {
		"@axe-core/playwright": "^4.9.0",
		"@lhci/cli": "^0.13.0",
		"@playwright/test": "^1.47.2",
		"@sentry/cli": "^2.53.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"@types/qrcode": "^1.5.5",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitest/coverage-v8": "^3.2.4",
		"autoprefixer": "^10.4.16",
		"dotenv-cli": "^7.4.4",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"jsdom": "^26.1.0",
		"postcss": "^8.4.32",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^3.2.4",
		"wait-on": "^7.2.0"
	},
	"dependencies": {
		"@sentry/node": "^10.10.0",
		"@sentry/sveltekit": "^10.10.0",
		"@stripe/stripe-js": "^7.9.0",
		"@supabase/ssr": "^0.7.0",
		"@supabase/supabase-js": "^2.39.0",
		"@tanstack/svelte-query": "^5.17.0",
		"blockhash-core": "^0.1.0",
		"exifr": "^7.1.3",
		"lucide-svelte": "^0.294.0",
		"qrcode": "^1.5.4",
		"sharp": "^0.34.3",
		"stripe": "^14.25.0",
		"uuid": "^9.0.1",
		"zod": "^3.22.4"
	},
	"type": "module"
}
